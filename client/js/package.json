{
	"name": "@gradio/client",
	"version": "1.8.0",
	"description": "Gradio API client",
	"type": "module",
	"main": "dist/index.js",
	"author": "",
	"license": "ISC",
	"exports": {
		".": {
			"gradio": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"fetch-event-stream": "^0.1.5",
		"semiver": "^1.1.0"
	},
	"devDependencies": {
		"esbuild": "^0.21.0",
		"msw": "^2.2.1",
		"typescript": "^5.0.0"
	},
	"scripts": {
		"bundle": "vite build --ssr",
		"generate_types": "tsc",
		"build": "pnpm bundle && pnpm generate_types",
